{
  "extends": ["airbnb", "airbnb/hooks", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "no-param-reassign": "off",
    "import/prefer-default-export": "off",
    "react/jsx-filename-extension": "off"
  }
}
